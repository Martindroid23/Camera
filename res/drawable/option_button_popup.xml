<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="true">
        <shape
          xmlns:android="http://schemas.android.com/apk/res/android">
            <solid android:color="@color/colorCameraBasicDoubleTransparent" />
            <corners android:radius="24.0dip" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape
          xmlns:android="http://schemas.android.com/apk/res/android">
            <solid android:color="@color/colorCameraBasicDoubleTransparent" />
            <corners android:radius="24.0dip" />
        </shape>
    </item>
    <item>
        <shape
          xmlns:android="http://schemas.android.com/apk/res/android">
            <solid android:color="@color/colorCameraBasicDoubleTransparent" />
            <corners android:topRightRadius="0.0dip" android:bottomRightRadius="0.0dip" />
        </shape>
    </item>
</selector>